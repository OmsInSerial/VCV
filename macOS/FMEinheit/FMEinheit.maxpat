, 							{
								"box" : 								{
									"code" : "History pot_pos(0.01); // default position + slight offset to always be > 0\r\nHistory pot_pos_shifted; // default position + slight offset to always be > 0\r\nHistory position; // position computation to be outputed to out1\r\nHistory position_shifted; // shifted position computation to be outputed to out2\r\nHistory pot_pos_reached(1); // actual pot pos == backed up pot pos \r\nHistory pot_pos_shifted_reached; // shifted pot pos == backed up shifted pot pos \r\nHistory intensity_r;\r\nHistory intensity_g;\r\nactual_pot_pos = in1; // live pot position\r\ncontrol_signal = in2; // control signal can be cv, button ...\r\nrate = 0.1; // tolerance\r\n\r\nif (control_signal == 1) { // control signal detection\r\n   pot_pos_reached = 0;\r\n   // position_shifted = pot_pos_shifted + actual_pot_pos; \r\n   intensity_g = 0;\r\n   if (pot_pos_shifted_reached){ // reached backep up shifted pot pos\r\n      position_shifted = actual_pot_pos; \n      pot_pos_shifted_reached = 1;\r\n      pot_pos_reached = 0; \n      intensity_r = 0;\n      intensity_g = 1; \n   }\r\n   else if (actual_pot_pos > (pot_pos_shifted + rate*pot_pos_shifted)){ // above backep up shifted pot pos\n      pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\n\t  //position = pot_pos + pot_pos_shifted*control_signal; \n      intensity_r = (1 - actual_pot_pos)/(1 - pot_pos_shifted); \n      intensity_g = 0;\n   }\n   else if (actual_pot_pos < (pot_pos_shifted - rate*pot_pos_shifted)){ // under backep up shifted pot pos\n\t  pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\n\t  //position = pot_pos + pot_pos_shifted*control_signal; \n      intensity_r = actual_pot_pos/pot_pos_shifted;\n      intensity_g = 0;\n   }\n   else { // equal backep up shifted pot pos\r\n      pot_pos_shifted_reached = 1;\r\n      pot_pos_reached = 0;\n\t  //position = pot_pos + pot_pos_shifted*control_signal; \n      intensity_r = 0;\n      intensity_g = 1;\n   };\r\n}\r\nelse {\r\n   pot_pos_shifted_reached = 0;\r\n   intensity_g = 0;\r\n   if (pot_pos_reached){\r\n      position = actual_pot_pos; \r\n      pot_pos_reached = 1; \r\n      pot_pos_shifted_reached = 0;\r\n      intensity_r = 0;\r\n      intensity_g = 1;  \r\n   }\r\n   else if (actual_pot_pos > (pot_pos + rate*pot_pos)){\r\n      pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\r\n      position = pot_pos;\r\n      intensity_r = (1 - actual_pot_pos)/(1 - pot_pos);\r\n      intensity_g = 0;\r\n   }\r\n   else if (actual_pot_pos < (pot_pos - rate*pot_pos)){\r\n\t  pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\r\n\t  position = pot_pos;\r\n      intensity_r = actual_pot_pos/pot_pos;\r\n      intensity_g = 0;\r\n   }\r\n   else {\r\n      pot_pos_reached = 1;\r\n      pot_pos_shifted_reached = 0;\r\n      position = pot_pos; \r\n      intensity_r = 0;\r\n      intensity_g = 1;\r\n\r\n   };   \r\n};\r\n\r\npot_pos = latch(actual_pot_pos, pot_pos_reached);\r\npot_pos = clamp(pot_pos,0.01,1);\r\npot_pos_shifted = latch(actual_pot_pos, pot_pos_shifted_reached, init=0.5);\r\npot_pos_shifted = clamp(pot_pos_shifted, 0,1);\r\nout1 = position;\r\nout2 = position_shifted;\r\nout3 = intensity_r;\r\nout4 = intensity_g;",
