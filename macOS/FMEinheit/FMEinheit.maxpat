pot_pos_reached){\r\n      position = actual_pot_pos; \r\n      pot_pos_reached = 1; \r\n      pot_pos_shifted_reached = 0;\r\n      intensity_r = 0;\r\n      intensity_g = 1;  \r\n   }\r\n   else if (actual_pot_pos > (pot_pos + rate*pot_pos)){\r\n      pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\r\n      position = pot_pos;\r\n      intensity_r = (1 - actual_pot_pos)/(1 - pot_pos);\r\n      intensity_g = 0;\r\n   }\r\n   else if (actual_pot_pos < (pot_pos - rate*pot_pos)){\r\n\t  pot_pos_reached = 0;\r\n\t  pot_pos_shifted_reached = 0;\r\n\t  position = pot_pos;\r\n      intensity_r = actual_pot_pos/pot_pos;\r\n      intensity_g = 0;\r\n   }\r\n   else {\r\n      pot_pos_reached = 1;\r\n      pot_pos_shifted_reached = 0;\r\n      position = pot_pos; \r\n      intensity_r = 0;\r\n      intensity_g = 1;\r\n\r\n   };   \r\n};\r\n\r\npot_pos = latch(actual_pot_pos, pot_pos_reached, init=0.5);\r\npot_pos = clamp(pot_pos,0,1);\r\npot_pos_shifted = latch(actual_pot_pos, pot_pos_shifted_reached, init=0.5);\r\npot_pos_shifted = clamp(pot_pos_shifted, 0,1);\r\nout1 = position;\r\nout2 = position_shifted;\r\nout3 = intensity_r;\r\nout4 = intensity_g;",
